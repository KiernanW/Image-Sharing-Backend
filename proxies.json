{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "proxy1": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/Website/{index}"
            },
            "backendUri": "https://imagesharing.azurewebsites.net/api/Website?filePath={index}"
        },
        "proxy2": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/Website/js/{file}"
            },
            "backendUri": "https://imagesharing.azurewebsites.net/api/Website?filePath={file}"
        },
        "proxy3": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/Website/css/{file}"
            },
            "backendUri": "https://imagesharing.azurewebsites.net/api/Website?filePath={file}"
        }
    }
}